{
  "name": "discord-integrations",
  "version": "1.3.1",
  "description": "A server to manage multiple Discord integrations.",
  "main": "./prod/index.js",
  "scripts": {
    "prebuild": "rm -rf ./prod",
    "build": "tsc",
    "postbuild": "cp -r ./src/server/views ./prod/server/views",
    "lint": "eslint src",
    "start": "node -r dotenv/config ./prod/index.js",
    "test": "echo 'no tests yet'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/discord-integrations.git"
  },
  "keywords": [
    "discord",
    "typescript",
    "express",
    "twitter"
  ],
  "author": "Nicholas Carrigan",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/nhcarrigan/discord-integrations/issues"
  },
  "homepage": "https://github.com/nhcarrigan/discord-integrations#readme",
  "engines": {
    "node": ">=16.3.0",
    "npm": ">=7.15.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.10",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@sentry/integrations": "^6.10.0",
    "@sentry/node": "^6.10.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "winston": "^3.3.3"
  }
}
